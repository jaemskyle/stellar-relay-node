    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.108",
    "dotenv": "^16.4.5",
    "save": "^2.9.0",
    "web-vitals": "^2.1.4",
    "ws": "^8.18.0"
  },
  "eslintConfig": {
